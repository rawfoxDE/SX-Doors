
SCOPT=NOSTACKCHECK NOSTACKEXTEND SMALLCODE SMALLDATA DATA=NEAR CODE=NEAR \
      STRINGMERGE NOMULTIPLEINCLUDES NOICONS IDENTIFIERLENGTH=64

OOPExample: OOPExample.c OOPExample.h
    @Echo "*e[1;32mCompiling *"OOPExample_SAS*"...*e[0m"
    @sc OOPExample.c $(SCOPT) CPU=ANY NOOPTTIME OPTSIZE LINK TO OOPExample_SAS

