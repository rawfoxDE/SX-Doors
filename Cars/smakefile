.c.o:
		SC $(CFLAGS) $(CPU) $*.c

CFLAGS		= NMINC MCCONS STREQ STRMERGE NOSTKCHK DEFINE=__PROTO__
LFLAGS		= SC SD NOICONS STRIPDEBUG

CPU		= CPU=020

OBJS		= SXCARS.o finish.o
LIBS		= LIB:sc.lib LIB:amiga.lib

SXCARS:	$(OBJS)
		SLINK LIB:c.o $(OBJS) TO amix:sx/doors/sxcars/SXCARS LIB $(LIBS) $(LFLAGS)
